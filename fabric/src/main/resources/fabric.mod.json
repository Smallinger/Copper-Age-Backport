{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://github.com/Smallinger/Copper-Age-Backport",
    "sources": "https://github.com/Smallinger/Copper-Age-Backport",
    "issues": "https://github.com/Smallinger/Copper-Age-Backport/issues"
  },
  "license": "${license}",
  "icon": "logo.png",
  "environment": "*",
    "entrypoints": {
      "main": [
        "com.github.smallinger.copperagebackport.fabric.CopperAgeBackportFabric"
      ],
      "client": [
        "com.github.smallinger.copperagebackport.fabric.client.CopperAgeBackportFabricClient"
      ],
      "modmenu": [
        "com.github.smallinger.copperagebackport.fabric.client.ModMenuIntegration"
      ]
    },
  "mixins": [
    "copperagebackport.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_version}",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}"
  },
  "suggests": {
    "modmenu": "*",
    "fastchest": "*"
  }
}
